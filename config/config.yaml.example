instanceNumber: 1

# API consumers & credentials for clients using this service
apiConsumers:
    vpn-user-portal: uvwxyz
    vpn-admin-portal: uuvvww

# API providers & credentials for servers this service is connecting to
apiProviders:
    vpn-ca-api:
        userName: vpn-server-api
        userPass: aabbcc
        apiUri: 'http://localhost:8008/api.php' 

vpnPools:
    internet:
        extIf: eth0
        listen: 0.0.0.0
        range: 10.0.0.0/24
        range6: 'fd00:4242:4242::/48'
        useNat: true
        defaultGateway: true
        dns: [8.8.8.8, 8.8.4.4]
